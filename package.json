{
  "name": "objection-visibility",
  "version": "0.3.0",
  "description": "Objectionjs plugin to whitelist/blacklist model properties ",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "oscar",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run build",
    "lint": "tslint --project tsconfig.json",
    "prebuild": "rm -r dist && npm run lint",
    "build": "tsc",
    "pretest": "npm run lint",
    "test": "mocha -r ts-node/register --slow 10 --timeout 15000 --reporter spec test/test.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oscaroox/objection-visibility.git"
  },
  "bugs": {
    "url": "https://github.com/oscaroox/objection-visibility/issues"
  },
  "standard": {
    "ignore": [
      "test.js"
    ]
  },
  "keywords": [
    "objection",
    "objectionjs",
    "objection-visibilty",
    "orm",
    "plugin",
    "whitelist",
    "blacklist"
  ],
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/knex": "^0.14.14",
    "@types/lodash.omit": "^4.5.3",
    "@types/lodash.pick": "^4.4.3",
    "@types/mocha": "^5.2.3",
    "@types/node": "^10.5.0",
    "chai": "^4.1.1",
    "knex": "^0.13.0",
    "mocha": "^3.5.0",
    "objection": "^1.1.10",
    "sqlite3": "4.0.1",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "lodash.omit": "~4.5.0",
    "lodash.pick": "~4.4.0"
  },
  "peerDependencies": {
    "objection": ">= 0.8.6 < 2"
  }
}
